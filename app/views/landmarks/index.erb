<h2>All Historical Landmarks</h2>


<% @landmarks.each do |landmark| %>
    <label>Name</label><li><a href="/landmarks/<%= landmark.id %>"><%= landmark.name %></a></li>
    <label>Year Completed</label><li><%= landmark.year_completed %></li><br><br>
<% end %>

<a class="button" href="/landmarks/new">Create New Landmark</a>
